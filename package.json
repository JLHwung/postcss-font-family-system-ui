{
  "name": "postcss-font-family-system-ui",
  "version": "2.0.1",
  "description": "PostCSS plugin to transform W3C CSS font-family: system-ui to a practical font-family list",
  "keywords": [
    "css",
    "postcss",
    "postcss-plugin",
    "font-family",
    "system-ui"
  ],
  "main": "src/index.js",
  "module": "src/main.mjs",
  "jsnext:main": "src/main.mjs",
  "repository": "https://github.com/JLHwung/postcss-font-family-system-ui.git",
  "author": "Junliang Huang <i@jhuang.me>",
  "license": "MIT",
  "scripts": {
    "lint": "standard",
    "test": "cross-env NODE_ENV=test nyc tap test/**/*.test.js && npm run spellcheck",
    "coverage": "nyc report",
    "spellcheck": "yaspeller-ci .",
    "prepublishOnly": "npm run lint && npm run test"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "postcss": "^6.0.1",
    "postcss-value-parser": "^3.3.0"
  },
  "devDependencies": {
    "@std/esm": "^0.15.0",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-preset-env": "^1.5.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.0.0",
    "nyc": "^11.0.2",
    "pre-commit": "^1.2.2",
    "standard": "^10.0.0",
    "tap": "^10.6.0",
    "yaspeller-ci": "^0.7.0"
  },
  "pre-commit": "lint",
  "@std/esm": {
    "esm": "all",
    "cjs": true
  }
}
