{
  "name": "postcss-font-family-system-ui",
  "version": "1.0.2",
  "description": "PostCSS plugin to transform W3C CSS font-family: system-ui to a practical font-family list",
  "keywords": [
    "css",
    "postcss",
    "postcss-plugin",
    "font-family",
    "system-ui"
  ],
  "main": "lib/index.js",
  "repository": "https://github.com/JLHwung/postcss-font-family-system-ui.git",
  "author": "Junliang Huang <i@jhuang.me>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf lib",
    "build": "babel src --out-dir lib",
    "lint": "standard",
    "test": "cross-env NODE_ENV=test nyc tap test/**/*.test.js && npm run spellcheck",
    "coverage": "nyc report",
    "spellcheck": "yaspeller .",
    "prepublishOnly": "npm run lint && npm run test && npm run clean && npm run build"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "postcss": "^5.2.12",
    "postcss-value-parser": "^3.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-preset-env": "^1.1.4",
    "babel-register": "^6.22.0",
    "cross-env": "^3.2.3",
    "nyc": "^10.0.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.5.4",
    "standard": "^8.6.0",
    "tap": "^10.0.0",
    "yaspeller": "^3.0.0"
  },
  "pre-commit": "lint"
}
